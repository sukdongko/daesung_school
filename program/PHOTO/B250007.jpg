    	else if (document.layers) {
    		document.div_pop3.visibility="hide"
    		//clearTimeout(tid);
    	}
    } //end function



/* -------------------------------------------------------------------------
//	공통함수
------------------------------------------------------------------------- */
    if (document.all){
    	document.onmouseup=new Function("p1_dragapproved=false;p2_dragapproved=false;p3_dragapproved=false;")
    	//document.onmouseup=new Function("p2_dragapproved=false")
