    var p3_dragswitch=0;
    var p3_nsx;
    var p3_nsy;
    //var p3_nstemp;
    var p3_dragapproved=false;

    function p3_drag_dropns(name){
        p3_temp=eval(name)
        p3_temp.captureEvents(Event.MOUSEDOWN | Event.MOUSEUP)
        p3_temp.onmousedown	=p3_gons
        p3_temp.onmousemove	=p3_dragns
        p3_temp.onmouseup	=p3_stopns

    }

    function p3_gons(e){
        p3_temp.captureEvents(Event.MOUSEMOVE)
        p3_nsx=e.x
        p3_nsy=e.y
    }

    function p3_dragns(e){
        if (p3_dragswitch==1){
            p3_temp.moveBy(e.x-p3_nsx,e.y-p3_nsy)
            return false
        }
    }

