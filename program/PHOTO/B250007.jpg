<script language="javascript">

/* -------------------------------------------------------------------------
//	LAYER POPUP 1
------------------------------------------------------------------------- */
    var p1_dragswitch=0;
    var p1_nsx;
    var p1_nsy;
    //var p1_nstemp;
    var p1_dragapproved=false;

    function p1_drag_dropns(name){
        p1_temp=eval(name)
        p1_temp.captureEvents(Event.MOUSEDOWN | Event.MOUSEUP)
        p1_temp.onmousedown	=p1_gons
        p1_temp.onmousemove	=p1_dragns
        p1_temp.onmouseup	=p1_stopns
    }

    function p1_gons(e){
        p1_temp.captureEvents(Event.MOUSEMOVE)
        p1_nsx=e.x
        p1_nsy=e.y
    }

    function p1_dragns(e){
        if (p1_dragswitch==1){
            p1_temp.moveBy(e.x-p1_nsx,e.y-p1_nsy)
            return false
        }
    }

    function p1_stopns(){
        p1_temp.releaseEvents(Event.MOUSEMOVE)
    }

    function p1_drag_dropie(){

        if (p1_dragapproved==true){
            document.all.div_pop1.style.pixelLeft	=p1_tempx+event.clientX-p1_iex
            document.all.div_pop1.style.pixelTop	=p1_tempy+event.clientY-p1_iey
            return false
        }
    }

    function p1_initializedragie(){
    	div_pop1.style.zIndex = 30;
    	div_pop2.style.zIndex = 20;
    	div_pop3.style.zIndex = 20;

        p1_iex=event.clientX
        p1_iey=event.clientY
        p1_tempx=div_pop1.style.pixelLeft
        p1_tempy=div_pop1.style.pixelTop
        p1_dragapproved=true
        document.onmousemove=p1_drag_dropie
    }


    //var tid =setTimeout(initializedragie(),1000);

    function p1_hidebox()
    {
    	var SILayer = document.getElementById('div_pop1');
    	if (typeof(SILayer)=='object')  {
    		SILayer.style.visibility="hidden"

    		//clearTimeout(tid);

    	}
    	else if (document.layers) {
    		document.div_pop1.visibility="hide"
    		//clearTimeout(tid);
    	}
    } //end function


